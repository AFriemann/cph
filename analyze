#!/bin/sh

if [ $(basename $(pwd)) == "analysis" ]; then
  cd ..
fi

logfile=analysis/$(date +'%d-%m-%y').log

echo "Printing $1 generated keys for version: $(./cph --version)" >> $logfile

for i in `seq 1 $1`;
do
  profile=$(pwgen -N1)
  password=$(pwgen -N1)
  echo -e "\t$profile $password: $(./cph --print-key -p $password $profile)" >> $logfile
done
